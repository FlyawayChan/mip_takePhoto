/* pages/my/my.wxss */
page{
  height: 100%;
}
.imgbox {
   width: 100%;
   height: 100%;
   column-count: 3; 
   column-gap: 5rpx;
   background-color: rgb(235, 235, 245)
}

.imgline{
    width: 100%;
    position: relative;
}

image {
    width: 100%;
    height: 250rpx;
    margin-top: 5px;
    border-radius: 15rpx;
}

.cancelI{
    position: absolute;
    right: 3px;
    left: auto;
    top: 8px;
}

.item_txt{
    position: absolute;
    bottom: 0px;
    width: 100%;
    height: 25px;
    left: 0px;
    background-color: #999;
    color: white;
    font-style: 13px;
    text-align: center;
}