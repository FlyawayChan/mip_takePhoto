	/***
	 * 获取预定日期到上个月的年月时间段，返回格式yyyyMM
	 */
	public  static List<String> getMonthInfo(String synStartTime){
		Date date=new Date();
		int year=Integer.valueOf(synStartTime.split("-")[0]);
		int month=Integer.valueOf(synStartTime.split("-")[1]);
		int year1=date.getYear()+1900;
		int month1=date.getMonth()+1;
		List<String> list=new ArrayList<String>();
		for(int i=(year*12+month);i<(year1*12+month1);i++){
			System.out.println(year+""+(month<10?"0"+month:""+month));
			list.add(year+""+(month<10?"0"+month:""+month));
			if(month>12){
				month=1;
				year++;
			}
			month++;
		}
		return list;
	}


